{
  "name": "socialflyai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:start": "docker run --name socialflyai-db -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=socialflyai -p 5432:5432 -d postgres:15",
    "db:stop": "docker stop socialflyai-db && docker rm socialflyai-db",
    "db:status": "docker ps | findstr socialflyai-db",
    "db:setup": "npm run db:start && sleep 5 && npx prisma db push && npx prisma generate",
    "db:reset": "npm run db:stop && npm run db:setup",
    "db": "node db-manager.js",
    "scheduler": "curl -X POST http://localhost:3000/api/scheduler/process",
    "scheduler:remote": "curl -X POST https://socialflyai-mkkv.vercel.app/api/scheduler/process",
    "lint": "eslint"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.6.5",
    "form-data": "^4.0.0",
    "googleapis": "^128.0.0",
    "next": "16.1.1",
    "pg": "^8.17.2",
    "prisma": "^7.2.0",
    "querystring": "^0.2.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
